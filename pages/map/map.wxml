<view class="page-body" bindtap='f1'>
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 300px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      show-location
    ></map>
  </view>
<view class="page-section">
    
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <input focus='true' class="weui-input" placeholder="输入美食" type='text' confirm-type='serch'/>
      </view>
    </view>
  </view>
</view>
